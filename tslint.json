{
    "rules": {
        "no-unreachable": true,
        "no-bitwise": true,
        "indent": [
            true,
            "spaces"
        ],
        "object-literal-sort-keys": true,
        "class-name": true,
        "no-unused-variable": [
            true
        ],
        "eofline": true,
        "no-trailing-whitespace": true,
        "use-isnan": true,
        "triple-equals": [
            true,
            "allow-null-check"
        ],
        "no-reference": true,
        "adjacent-overload-signatures": true,
        "member-ordering": [
            true,
            {
                "order": "fields-first"
            }
        ],
        "no-namespace": true,
        "typedef": [
            true,
            "call-signature",
            "parameter",
            "arrow-parameter"
        ],
        "typedef-whitespace": [
            true,
            {
                "call-signature": "nospace",
                "index-signature": "nospace",
                "parameter": "nospace",
                "property-declaration": "nospace",
                "variable-declaration": "nospace"
            },
            {
                "call-signature": "onespace",
                "index-signature": "onespace",
                "parameter": "onespace",
                "property-declaration": "onespace",
                "variable-declaration": "onespace"
            }
        ],
        "curly": true,
        "no-empty": true,
        "no-duplicate-key": true,
        "no-duplicate-variable": true,
        "radix": true,
        "align": [
            true,
            "parameters",
            "statements"
        ],
        "arrow-parens": true,
        "jsdoc-format": true,
        "new-parens": true,
        "interface-name": [
            true,
            "always-prefix"
        ],
        "no-angle-bracket-type-assertion": true,
        "no-constructor-vars": true,
        "object-literal-key-quotes": [
            true,
            "as-needed"
        ],
        "ordered-imports": true,
        "no-consecutive-blank-lines": true,
        "one-variable-per-declaration": true,
        "one-line": [
            true,
            "check-catch",
            "check-finally",
            "check-else",
            "check-else",
            "check-open-brace",
            "check-whitespace"
        ],
        "quotemark": [
            true,
            "single",
            "avoid-escape"
        ],
        "semicolon": [
            true,
            "never"
        ],
        "variable-name": [
            true,
            "ban-keywords",
            "check-format"
        ],
        "whitespace": [
            true,
            "check-branch",
            "check-module",
            "check-separator",
            "check-separator",
            "check-decl",
            "check-typecast",
            "check-type"
        ]
    }
}